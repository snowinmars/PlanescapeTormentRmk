    # nr "Дух протягивает руки."
    nr ""

# game/dlgs/deionarra.rpy:90
translate english deionarra_s3_3fc554e4:

    # deionarra_unknown "«Подумай…»"
    deionarra_unknown ""

# game/dlgs/deionarra.rpy:91
