# game/dlgs/soego.rpy:1314
translate english soego_s59_27443530:

    # soego "«Ты нашел то, что искал?»{#soego_s59_2}"
    soego "«Did you find what you were looking for?»{#soego_s59_2}"

# game/dlgs/soego.rpy:1332
