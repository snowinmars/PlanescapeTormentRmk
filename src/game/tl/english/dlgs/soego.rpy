# game/dlgs/soego.rpy:41
translate english soego_s1_57d1a921:

    # "soego_s1{#soego_s1}"
    nr "The Dustman's head snaps up as you address him.{#soego_s1_1}"
    soego "«Par… pardon? Did you just speak to me?»{#soego_s1_2}"

# game/dlgs/soego.rpy:71
translate english soego_s2_f1b825f6:

    # "soego_s2{#soego_s2_1}"
    soego "«By the powers!»{#soego_s2_1}"
    nr "The Dustman jumps, then stares intently at you. You notice his eyes aren't bloodshot as much as they have a red tinge to them.{#soego_s2_2}"
    soego "«Sirrah, you force an unflattering confession from me: you make a convincing zombie».{#soego_s2_3}"
    nr "He bows slightly.{#soego_s2_4}"

# game/dlgs/soego.rpy:73
translate english soego_s2_090722d5:

    # "soego_s2{#soego_s2_2}"
    soego "«I am Soego. May I ask your business here…»{#soego_s2_5}"
    nr "He glances at your scars.{#soego_s2_6}"
    soego "«…looking like that?»{#soego_s2_7}"

# game/dlgs/soego.rpy:105
translate english soego_s3_61636e1b:

    # "soego_s3{#soego_s3}"
    nr "The Dustman studies you for a moment, then shakes his head.{#soego_s3_1}"
    soego "«More imaginings…»{#soego_s3_2}"
    nr "He sighs and rubs his eyes.{#soego_s3_3}"
    soego "«These fever spells are getting worse…»{#soego_s3_4}"

# game/dlgs/soego.rpy:134
translate english soego_s4_7932a577:

    # "soego_s4{#soego_s4}"
    nr "The Dustman looks carefully at you, then leans in… his lips peel back, revealing a row of dirty, sharp teeth, and he begins to sniff at you like a rat.{#soego_s4_1}"

# game/dlgs/soego.rpy:165
