    nr 'Они определенно кем-то сделаны, но для чего — непонятно.{#copearc_s1_2}'

# game/dlgs/inventory/copearc.rpy:50
translate russian copearc_s2_2de847c6:

    # 'copearc_s2{#copearc_s2}'
